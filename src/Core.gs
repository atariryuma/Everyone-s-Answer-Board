      nameItem.setHelpText(config.nameQuestion.helpText);
      nameItem.setRequired(false);
    }

    debugLog('ãƒ•ã‚©ãƒ¼ãƒ ã«çµ±ä¸€è³ªå•ã‚’è¿½åŠ ã—ã¾ã—ãŸ: ' + questionType);
    
