    } catch (repairError) {
      console.error('getSheetsListInternal: æ¨©é™ä¿®å¾©ã«å¤±æ•—:', repairError.message);
      return {
        error: true,
        message: 'ã‚¹ãƒ—ãƒ¬ãƒƒãƒ‰ã‚·ãƒ¼ãƒˆã¸ã®ã‚¢ã‚¯ã‚»ã‚¹æ¨©é™ãŒã‚ã‚Šã¾ã›ã‚“ã€‚ç®¡ç†è€…ã«ãŠå•ã„åˆã‚ã›ãã ã•ã„ã€‚',
