    // ãƒ•ã‚©ãƒ¼ãƒ URLã®å–å¾—ã¨ä¿®å¾©
    let formUrl = configJson.formUrl || configJson.editFormUrl;
    
    // ãƒ•ã‚©ãƒ¼ãƒ URLãŒãªã„å ´åˆã€ã‚¹ãƒ—ãƒ¬ãƒƒãƒ‰ã‚·ãƒ¼ãƒˆã‹ã‚‰å–å¾—ã‚’è©¦ã¿ã‚‹
    if (!formUrl && userInfo.spreadsheetId && configJson.formCreated) {
      try {
