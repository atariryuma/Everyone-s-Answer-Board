function checkSpreadsheetSharingPermission(spreadsheetId) {
  try {
    var service = getDriveService(); // Drive APIã‚µãƒ¼ãƒ“ã‚¹ã‚’å–å¾—
    var file = service.files.get({
      fileId: spreadsheetId,
      fields: 'permissions'
